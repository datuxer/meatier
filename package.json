{
  "name": "meatier",
  "version": "0.10.0",
  "description": "Like meteor, but meatier",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattkrick/meatier.git"
  },
  "bugs": {
    "url": "https://github.com/mattkrick/meatier/issues"
  },
  "main": "index.js",
  "scripts": {
    "start": "set NODE_ENV=development && node ./src/server/server.babel.js",
    "prod": "set NODE_ENV=production && node ./src/server/server.babel.js",
    "build": "rimraf build && concurrently \"npm run build:client\" \"npm run build:server\"",
    "bs": "rimraf build && concurrently \"npm run build:client\" \"npm run build:server\" \"npm run prod\"",
    "quickstart": "rimraf build && concurrently \"npm run build:db\" \"npm run build:client\" \"npm run build:server\" \"npm run prod\"",
    "build:client": "set NODE_ENV=production && webpack --config ./webpack/prod.babel.js",
    "build:server": "set NODE_ENV=production && webpack --config ./webpack/server.babel.js",
    "build:db": "node ./src/server/database/setupDB.babel.js",
    "update:db": "node ./src/server/database/setupDB.babel.js true",
    "test": "set NODE_ENV=testing && ava ./src/**/__tests__/**/*.js --verbose",
    "lint": "xo --fix 'src/**/*.js' 'tests/*.js' 'webpack/*.js'",
    "profile": "set NODE_ENV=production && webpack --config ./webpack/prod.babel.js --profile --json > stats.json"
  },
  "author": "Matt Krick <matt.krick@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "assets-webpack-plugin": "3.5.0",
    "ava": "0.17.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.8",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-static-fs": "1.1.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "concurrently": "3.1.0",
    "css-loader": "0.26.1",
    "eslint": "3.11.1",
    "eslint-config-xo-meatier": "0.2.2",
    "eslint-config-xo-react": "0.10.0",
    "eslint-plugin-babel": "4.0.0",
    "eslint-plugin-react": "6.8.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "fake-style-loader": "1.0.1",
    "file-loader": "0.9.0",
    "happypack": "3.0.1",
    "json-loader": "0.5.4",
    "null-loader": "0.1.1",
    "postcss-loader": "1.2.0",
    "postcss-modules-values": "1.2.2",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.3.3",
    "rimraf": "2.5.4",
    "stats-webpack-plugin": "0.4.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.27",
